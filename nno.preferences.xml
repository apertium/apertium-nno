  <incompatibilities>
    <incompat c="ikkje fylgje, bylgje viss følge, bølge er vald">
      <pref n="følgje_fylgje.vok-ø_gj2y_gj" v="false"/>
      <pref n="følgje_følge.kons-lgj2lg" v="true"/>
    </incompat>
    <incompat c="berre fylgje, bylgje viss fyrst, ynske er vald">
      <pref n="følgje_fylgje.vok-ø_gj2y_gj" v="true"/>
      <pref n="først_fyrst.vok-ø2y" v="false"/>
    </incompat>
    <incompat c="ikkje flyr (pres) viss flyge (lang infinitiv) er vald">
      <pref n="fly_flyge.vb-inf" v="false"/>
      <pref n="flyg_flyr.vb-inf" v="true"/>
    </incompat>
    <incompat c="ikkje forklarar (pres) viss forklårar er vald">
      <pref n="brukte_bruka.vb-e2a" v="false"/>
      <pref n="allmenn_ålmenn.vok-a2å" v="true"/>
    </incompat>
  </incompatibilities>
